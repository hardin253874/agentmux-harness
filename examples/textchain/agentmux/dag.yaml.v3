# Gemini (first write â€” overwrite/create)
- id: "write:1-gemini"
  agent: "gemini"
  deps: []
  payload:
    kind: "shell"
    repoPath: "."
    cmd: >
      powershell -NoLogo -Command
      ./ai/gemini-cli/bin/gemini.ps1
      -model "gemini-2.5-pro"
      -prompt "Output exactly one line: 'Gemini line: <short random phrase>  [<ISO-8601 timestamp>]' and nothing else."
      -out "C:\Developments\Develop\MyProjects\agentmux-harness\artifacts\text\test.txt"
      -log "C:\Developments\Develop\MyProjects\agentmux-harness\artifacts\logs\gemini.txt"

